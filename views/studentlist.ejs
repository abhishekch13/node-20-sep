<%- include('./base/header.ejs')%>
<div class="container">
    <div class="row">
        <table class ="table text-center">
            <h3 class="text-center bg-success my-1" style="color: whitesmoke"> STUDENT LIST</h3>
            <thead>
                <tr>
                    <th>Roll No</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Father Name</th>
                    <th>Aadhar Card</th>
                    <th>Mobile No</th>
                    <th>Action</th>

                    

                </tr>
            </thead>
            <% students.forEach((student)=>{ %>
<tr>
    <td><%= student.rollNo %></td>
    <td><%= student.firstName %></td>
    <td><%= student.lastName %></td>
    <td><%= student.fatherName %></td>
    <td><%= student.aadharcardNo %></td>
    <td><%= student.mobileNo %></td>
    <td>
         <a href="/edit/student/page/<%= student._id%>" class="btn btn-warning w-40">Edit</a>
         <a href="/delete/student/<%= student._id%>" class="btn btn-danger w-40">Delete</a>
    </td>


</tr>
             <%})%>

        </table>  
      </div>
</div>
<%- include('./base/footer.ejs')%>
